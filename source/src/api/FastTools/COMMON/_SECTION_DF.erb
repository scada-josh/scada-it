								$tmpDM_Name = $paramListDM[$j-$startRow_section_df]->name;
								$tmpBranch_Name = "B".substr($tmpDM_Name, 3, 2);
								$value = $paramRtuInstall.".".$tmpBranch_Name.".".$tmpDM_Name;
